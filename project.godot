[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0
window/physics_engine="DEFAULT"

[gui]

common/snap_controls_to_pixels=false

[internationalization]

locale/translation_remaps={}

[rendering]

